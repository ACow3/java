package com.codingdojo.lookify.models;

@Entity
@Table(name="lookify")
public class Lookify {
	@Id
	@GeneratedValue(stradegy = Generation.Type.IDENTITY)
	private Long id;
	@Size(min = 5, max = 200)
	private String name;
	@Size(min = 5, max = 200)
	private String author;
	@Size(min = 5, max = 200)
	private int rating;
	
	@Column(updated=false)
	@DateTimeFormat(pattern="yyyy-MM-dd")
	private Date createdAt;
	@DateTimeFormat(pattern="yyyy-MM-dd")
	private Date udpatedAt;
	private String name;
	private String author;
	private int rating;
	
	
	public Lookify() {
	}
	
	public Lookify
}
